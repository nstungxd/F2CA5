<div class="modal fade " tabindex="-1" role="dialog" id="modalUploadFile" data-backdrop="static" data-keyboard="false">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                <h4 class="modal-title">Nạp dữ liệu</h4>
            </div>
            <div class="modal-body">
                <form id="fileupload" method="POST" class="form-inline" enctype="multipart/form-data">
                    <div class="row fileupload-buttonbar">
                        <div class="col-lg-12">
                            <div style="display:none">
                                <input type="hidden" name="dotId" value="@Model.DotKeKhai.ID" />
                                <input type="hidden" name="resultStatus" id="resultStatus" value="0" />
                            </div>
                            <select id="upload-data_import_type" name="importType" class="form-control input-sm">
                                <option value="1">Import thêm</option>
                                <option value="0">Xóa dữ liệu cũ</option>
                            </select>
                            <span class="btn btn-xs btn-info fileinput-button">
                                <i class="glyphicon glyphicon-file"></i>
                                <span>Chọn File</span>
                                <input name="file" type="file" multiple />
                            </span>
                        </div>
                    </div>
                    <br />
                    <div class="progress" id="progress">
                        <div class="progress-bar progress-bar-primary progress-bar-striped" role="progressbar" aria-valuenow="0.01" aria-valuemin="0" aria-valuemax="100" style="width: 0">
                        </div>
                    </div>
                    <table role="presentation" id="table-upload" class="table table-bordered table-hover">
                        <thead>
                            <tr>
                                <th>Tên tệp tin</th>
                                <th style="width:100px;">Dung lượng</th>
                                <th style="width:120px;"></th>
                            </tr>
                        </thead>
                        <tbody class="body-table-upload"></tbody>
                    </table>
                </form>
            </div>

            <div class="modal-footer">
                <button type="button" class="btn btn-xs btn-primary" onclick="model.UploadFileModel.uploadAll()">
                    <i class="glyphicon glyphicon-upload"></i>
                    <span>Nạp dữ liệu</span>
                </button>
                @*<button type="button" class="btn btn-xs btn-danger cancel" onclick="model.UploadFileModel.clearUpload()">
                        <i class="glyphicon glyphicon-ban-circle"></i>
                        <span>Hủy bỏ</span>
                    </button>*@

                <button type="button" class="btn btn-default btn-sm" data-dismiss="modal">
                    <b>Đóng</b>
                </button>
            </div>
        </div>
    </div>
</div>


